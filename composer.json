{
    "name": "railt/storage",
    "type": "library",
    "description": "The Railt Framework storage bridges and implementations.",
    "license": "MIT",
    "keywords": [
        "storage",
        "psr-6",
        "psr-16",
        "cache",
        "persisting"
    ],
    "homepage": "http://railt.org",
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/storage"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "psr/cache": "~1.0",
        "psr/simple-cache": "~1.0",
        "railt/io": "1.2.*|dev-master"
    },
    "autoload": {
        "psr-4": {
            "Railt\\Storage\\": "src"
        },
        "files": [
            "src/polyfill.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "codedungeon/phpunit-result-printer": "~0.16"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Tests\\Storage\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "suggest": {
        "psr/cache": "PSR-6 Cache Implementation",
        "psr/simple-cache": "PSR-16 Cache Implementation"
    },
    "provide": {
        "psr/cache": "~1.0",
        "psr/simple-cache": "~1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
