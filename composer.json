{
    "name": "railt/storage",
    "license": "MIT",
    "type": "library",
    "homepage": "http://railt.org",
    "description": "The Railt Framework storage bridges and implementations.",
    "keywords": [
        "storage",
        "psr-6",
        "psr-16",
        "cache",
        "persisting"
    ],
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/storage"
    },
    "require": {
        "php": "^7.1.3",
        "psr/cache": "~1.0",
        "psr/simple-cache": "~1.0",
        "railt/io": "1.4.*|1.4.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Railt\\": "src/"
        },
        "files": [
            "./src/polyfill.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Tests\\Storage\\": "tests/"
        }
    },
    "provide": {
        "psr/cache": "~1.0",
        "psr/simple-cache": "~1.0"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
