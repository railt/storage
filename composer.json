{
    "name": "railt/storage",
    "type": "library",
    "description": "The Railt Framework storage bridges and implementations.",
    "license": "MIT",
    "keywords": [ "storage", "psr-6", "psr-16", "cache", "persister" ],
    "homepage": "http://railt.org",
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/railt"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "railt/io": "~1.0|dev-master",
        "psr/log": "~1.0",
        "psr/cache": "~1.0",
        "psr/simple-cache": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Railt\\Storage\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "suggest": {
        "psr/cache-implementation": "PSR-6 Cache Implementation",
        "psr/simple-cache-implementation": "PSR-16 Cache Implementation"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
